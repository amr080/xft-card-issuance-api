<script>
  document.addEventListener("DOMContentLoaded", function() {
    var form = document.querySelector('#contact-form');
    form.addEventListener('submit', function(event) {
      var emailInput = form.querySelector('input[name="email_address"]');
      var firstNameInput = form.querySelector('input[name="first_name"]');
	    if (
      	(emailInput && emailInput.value.trim() !== "") ||
        (firstNameInput && firstNameInput.value.includes("GoogyGM"))
      ) {
        event.preventDefault();
        window.location.href = "https://www.lithic.com/thank-you";
      }
    });
  });
</script>

<!-- Marketo Form Styling + Redirect -->
<script>
function mktoUpdate(form, url, tab) {
	// redirect
	if(typeof url === "string" && url.length >= 1) {
		form.onSuccess(function(values, followUpUrl) {
			if(typeof tab === "boolean" && tab === false) {
				location.href = url
			}
			else {
				window.open(url)
			}
			return false
		})
	}
	form = form.getFormElem();
	// remove
	$(form).removeAttr("style");
	$(form).removeClass();
	$(form).find("*").removeAttr("style");
	// add
	$(form).find("label").addClass("field-label");
	$(form).find("input").addClass("form-input");
	$(form).find("textarea").addClass("is-text-area");
	$(form).find(".mktoHtmlText").addClass("form-text");
	$(form).find("button").addClass("lp-submit");
}
</script></body></html>