<script>
function mktoUpdate(form, url, tab) {
	// redirect
	if(typeof url === "string" && url.length >= 1) {
		form.onSuccess(function(values, followUpUrl) {
			if(typeof tab === "boolean" && tab === false) {
				location.href = url
			}
			else {
				window.open(url)
			}
			return false
		})
	}
	form = form.getFormElem();
	// remove
	$(form).removeAttr("style");
	$(form).removeClass();
	$(form).find("*").removeAttr("style");
	// add
	$(form).find("label").addClass("form-label-mkto");
	$(form).find("input").addClass("sf-form-input");
	$(form).find("textarea").addClass("is-text-area");
	$(form).find(".mktoHtmlText").addClass("form-text");
	$(form).find("button").addClass("button");
}
</script>

<script>
MktoForms2.whenReady(function(form){
    form.onSuccess(function(values, followUpUrl){
        // Take the lead to a different page on successful submit, ignoring the form's configured followUpUrl
       // Get the current URL
		let currentUrl = window.location.href;
		let queryString = window.location.search;
		let params = new URLSearchParams(queryString);
		let paramValue = params.get("paramName");

		window.location = "/thank-you" + queryString;
        return false;
    });
});
</script></body></html>